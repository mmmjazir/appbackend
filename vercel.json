{
  "version":2,
  "builds": [
    {
        "src": "./server.js",
        "use": "@vercel/node"
    }
  ],
  "routes":[
    {
      "src": "/api/shops/.*",
      "dest": "./routes/shops.js"
    },
    {
      "src": "/api/medicines/.*",
      "dest": "./routes/medicines.js"
    },
    {
      "src": "/api/medicines/public",
      "dest": "./routes/medicines.js"
    },
    {
      "src": "/api/shops/public",
      "dest": "./routes/shops.js"
    },
    {
      "src": "/api/user/login",
      "dest": "./routes/user.js"
    },
    {
      "src": "/api/user/signup",
      "dest": "./routes/user.js"
    },
    {
      "src": "/api/shops/:id",
      "dest": "./routes/shops.js"
    },
    {
      "src": "/api/medicines/:id",
      "dest": "./routes/medicines.js"
    }
  ]
}